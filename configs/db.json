            "dbName": "wrkfree"
        },
        "cacheEnv": {
            "local": {
                "host": "127.0.0.1",
                "port": 6379,
                "options": {},
                "ttl": 3
            },
            "global": {
                "host": "127.0.0.1",
                "port": 6379,
                "options": {},
                "ttl": 3
            }
        },
        "searchEnv": {
            "host": "127.0.0.1:9200",
